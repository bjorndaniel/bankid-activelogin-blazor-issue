		<PackageReference Include="ActiveLogin.Authentication.BankId.AspNetCore.UAParser" Version="5.0.0" />
		<PackageReference Include="Microsoft.VisualStudio.Azure.Containers.Tools.Targets" Version="1.14.0" />
	</ItemGroup>
  <ItemGroup>
    <Folder Include="Certificates\" />
  </ItemGroup>
  <ItemGroup>
    <None Include="Areas\BankIdAuthentication\Views\BankId\_Layout.cshtml" />
  </ItemGroup>
  <ItemGroup>
    <Compile Update="Shared\Resources\CertificateResources.Designer.cs">
      <DesignTime>True</DesignTime>
